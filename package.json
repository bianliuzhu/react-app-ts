{
	"name": "react-app-ts",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"mock": "dotenv -e .env.mock craco start",
		"debug": "dotenv -e .env.debug craco start",
		"testing": "dotenv -e .env.testing craco start",
		"pretest": "dotenv -e .env.pretest craco start",
		"build": "dotenv -e .env.produce craco build",
		"test": "craco test --env=jsdom",
		"plop": "plop --plopfile plop/index.js",
		"eject": "react-scripts eject",
		"f2elint-scan": "f2elint scan",
		"f2elint-fix": "f2elint fix"
	},
	"dependencies": {
		"@loadable/component": "^5.15.0",
		"@reduxjs/toolkit": "^1.6.2",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.9.1",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"antd-mobile": "^5.0.0-rc.7",
		"axios": "^0.21.4",
		"husky": "^7.0.2",
		"nprogress": "^0.2.0",
		"p-min-delay": "^4.0.1",
		"qs": "^6.10.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.5",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"redux": "^4.1.1",
		"typescript": "^4.4.3",
		"web-vitals": "^1.1.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "f2elint commit-file-scan",
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --single-quote --write",
			"git add"
		]
	},
	"devDependencies": {
		"@craco/craco": "^6.4.3",
		"@types/qs": "^6.9.7",
		"@types/react-router-dom": "^5.3.0",
		"app-root-path": "^3.0.0",
		"babel-plugin-import": "^1.13.3",
		"craco-antd": "^1.19.0",
		"craco-less": "^1.20.0",
		"f2elint": "^1.1.1",
		"lint-staged": "^11.1.2",
		"mockplugin": "^1.1.1",
		"plop": "^2.7.6",
		"postcss": "^8.3.11",
		"postcss-pxtorem": "^5.1.1",
		"prettier": "^2.4.0",
		"purgecss-webpack-plugin": "^4.1.3",
		"terser-brunch": "^4.1.0"
	}
}

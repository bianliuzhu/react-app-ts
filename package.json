  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/react-router-dom": "^5.3.0",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "http-proxy-middleware": "^2.0.1",
    "react-app-rewired": "^2.1.8"
  }
}
